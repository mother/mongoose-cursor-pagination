      "post-receive": "npm install",
      "post-rewrite": "npm install",
      "pre-commit": "npm run lint && npm audit"
    }
  },
  "devDependencies": {
    "eslint-config-mother": "^2.0.15",
    "eslint-plugin-jest": "^23.9.0",
    "husky": "^1.3.1",
    "jest": "^27.0.6",
    "mongodb-memory-server": "^6.5.2",
    "mongoose": "^5.13.6"
  },
  "peerDependencies": {
